{
  "name":        "chaoticwave/grubworm",
  "description": "Laravel Data Burrower",
  "keywords":    [
    "generator",
    "reverse-engineer",
    "database",
    "laravel",
    "cli",
    "command"
  ],
  "license":     "MIT",
  "type":        "library",
  "extra":       {
    "branch-alias": {
      "dev-develop": "1.x-dev"
    }
  },
  "require":     {
    "dreamfactory/php-utils": "1.*@dev"
  },
  "require-dev": {
