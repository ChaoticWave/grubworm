    "barryvdh/laravel-ide-helper": "~2.0",
    "doctrine/dbal":               "~2.3",
    "way/generators":              "~3.0",
    "phpunit/phpunit":             "~4.0",
    "phpspec/phpspec":             "~2.1"
  },
  "autoload":    {
    "psr-4": {
      "ChaoticWave\\Utility\\Grubworm\\": "src/"
    }
  },
  "config":      {
    "preferred-install": "dist"
  }
}
